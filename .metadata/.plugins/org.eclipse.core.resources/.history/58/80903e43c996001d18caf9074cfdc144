package classe;

public class DataTeste {
	
	public static void main(String[] args) {
		
		/*
		 * instanciar duas datas
		 */
		
		Data aniversarioMamae = new Data();
		aniversarioMamae.dia = 18;
		aniversarioMamae.mes = 9;
		aniversarioMamae.ano = 1954;
		
		Data aniversarioVovo = new Data();
		aniversarioVovo.dia = 25;
		aniversarioVovo.mes = 2;
		aniversarioVovo.ano = 1928;
		
		System.out.printf("%d/%d/%d", aniversarioMamae.dia, aniversarioMamae.mes, aniversarioMamae.ano);
		System.out.printf("%d/%d/%d", aniversarioVovo.dia, aniversarioVovo.mes, aniversarioVovo.ano);
	}

}
