package oo.heranca;

public class Jogo {
	
	public static void main(String[] args) {
		
		Jogador monstro = new Monstro();
		monstro.x = 10;
		monstro.y = 10;
		
		Jogador j2 = new Heroi();
		j2.x = 10;
		j2.y = 11;
		
		
		
		System.out.println(monstro.atacar(j2));
		System.out.println(j2.vida);
		
	}

}
