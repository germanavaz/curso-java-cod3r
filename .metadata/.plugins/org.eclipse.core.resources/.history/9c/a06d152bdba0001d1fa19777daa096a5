package oo.heranca.desafio;

public class Teste {
	
	public static void main(String[] args) {
		
		Carro fusca = new Fusca();
		
		fusca.acelerar();
		fusca.acelerar();
		fusca.acelerar();
		fusca.acelerar();
		fusca.freiar();
		fusca.freiar();
		
		System.out.println(fusca.velocidadeAtual);
		
		Carro ferrari = new Ferrari();
		
		ferrari.acelerar();
		ferrari.freiar();
		
		System.out.println(ferrari.velocidadeAtual);
	}

}
