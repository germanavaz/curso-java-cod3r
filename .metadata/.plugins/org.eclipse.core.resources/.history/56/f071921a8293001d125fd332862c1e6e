package fundamentos;

public class AreaCircunferencia {
	
	public static void main(String[] args) {
		
		double raio = 3.4;
		final double PI = 3.14159; //constantes s√£o com letra maiuscula
		
		double area = PI * raio * raio;
		
		System.out.println(area);
		
		raio = 10;
		System.out.println(area);
	}

}
