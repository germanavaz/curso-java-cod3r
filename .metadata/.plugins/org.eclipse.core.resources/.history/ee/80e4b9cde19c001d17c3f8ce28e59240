package classe;

public class DesafioPessoa {
	
	String nomePessoa;
	double pesoPessoa;
	
	public DesafioPessoa(String nomePessoa, double pesoPessoa) {
		this.nomePessoa =  nomePessoa;
		this.pesoPessoa = pesoPessoa;
	}
	
	void comer (String nomeComida) {
		this.pesoPessoa = pesoPessoa + DesafioComida.pesoComida;
	}

}
